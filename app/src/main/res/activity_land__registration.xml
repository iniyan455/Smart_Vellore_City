<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".land_reg.Land_Registration">
    <com.rengwuxian.materialedittext.MaterialEditText
        android:id="@+id/et_zone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_8dp"
        android:focusable="true"
        android:hint="@string/licencezone"
        android:drawableRight="@drawable/ic_expand_more_black"
        android:inputType="text"
        android:textSize="@dimen/dimen_14sp"
        android:text="Vellore"
        app:met_clearButton="false"
        app:met_floatingLabel="highlight"
        app:met_primaryColor="@color/colorPrimaryDark" />
    <com.rengwuxian.materialedittext.MaterialEditText
        android:id="@+id/et_reg_ofz"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_8dp"
        android:focusable="false"
        android:hint="@string/subregoffice"
        android:drawableRight="@drawable/ic_expand_more_black"
        android:inputType="text"
        android:textSize="@dimen/dimen_14sp"
        app:met_clearButton="false"
        app:met_floatingLabel="highlight"
        app:met_primaryColor="@color/colorPrimaryDark" />
    <com.rengwuxian.materialedittext.MaterialEditText
        android:id="@+id/et_village"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_8dp"
        android:focusable="false"
        android:hint="@string/selectvillage"
        android:drawableRight="@drawable/ic_expand_more_black"
        android:inputType="text"
        android:textSize="@dimen/dimen_14sp"
        app:met_clearButton="false"
        app:met_floatingLabel="highlight"
        app:met_primaryColor="@color/colorPrimaryDark" />
    <com.rengwuxian.materialedittext.MaterialEditText
        android:id="@+id/et_srvy_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_8dp"
        android:focusable="false"
        android:hint="@string/serveyno"
        android:drawableRight="@drawable/ic_expand_more_black"
        android:inputType="text"
        android:textSize="@dimen/dimen_14sp"
        app:met_clearButton="false"
        app:met_floatingLabel="highlight"
        app:met_primaryColor="@color/colorPrimaryDark" />
    <com.rengwuxian.materialedittext.MaterialEditText
        android:id="@+id/et_sub_div_no"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_8dp"
        android:focusable="false"
        android:hint="@string/subdivname"
        android:drawableRight="@drawable/ic_expand_more_black"
        android:inputType="text"
        android:textSize="@dimen/dimen_14sp"
        app:met_clearButton="false"
        app:met_floatingLabel="highlight"
        app:met_primaryColor="@color/colorPrimaryDark" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ADD"
        android:padding="14dp"
        android:textStyle="bold"
        android:layout_margin="@dimen/dimen_8dp"
        android:id="@+id/tv_add"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@color/colorPrimary"
        android:textColor="@android:color/white"
        />


</LinearLayout>